{
  "info": {
    "name": "Outsourcing Platform API",
    "description": "软件开发外包平台 API 集合",
    "version": "1.0.0"
  },
  "variable": [
    {
      "key": "base_url",
      "value": "http://localhost:3000",
      "type": "default"
    },
    {
      "key": "token",
      "value": "",
      "type": "string"
    }
  ],
  "item": [
    {
      "name": "认证",
      "item": [
        {
          "name": "注册",
          "request": {
            "method": "POST",
            "url": "{{base_url}}/auth/register",
            "header": [
              {"key": "Content-Type", "value": "application/json"}
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"phone\": \"18888888888\",\n  \"password\": \"password123\",\n  \"role\": \"DEVELOPER\"\n}"
            }
          }
        },
        {
          "name": "登录",
          "request": {
            "method": "POST",
            "url": "{{base_url}}/auth/login",
            "header": [
              {"key": "Content-Type", "value": "application/json"}
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"phone\": \"18888888888\",\n  \"password\": \"password123\"\n}"
            }
          }
        },
        {
          "name": "获取个人资料",
          "request": {
            "method": "GET",
            "url": "{{base_url}}/auth/profile",
            "header": [
              {"key": "Authorization", "value": "Bearer {{token}}"}
            ]
          }
        }
      ]
    },
    {
      "name": "订单管理",
      "item": [
        {
          "name": "创建订单",
          "request": {
            "method": "POST",
            "url": "{{base_url}}/orders",
            "header": [
              {"key": "Content-Type", "value": "application/json"},
              {"key": "Authorization", "value": "Bearer {{token}}"}
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"title\": \"开发一个React Native应用\",\n  \"description\": \"需要开发一个电商类的移动应用\",\n  \"budget\": 5000.00,\n  \"deliveryDays\": 30,\n  \"category\": \"mobile_app\",\n  \"tags\": [\"React Native\", \"iOS\", \"Android\"]\n}"
            }
          }
        },
        {
          "name": "获取订单列表",
          "request": {
            "method": "GET",
            "url": "{{base_url}}/orders?status=OPEN&page=1&limit=20"
          }
        },
        {
          "name": "获取订单详情",
          "request": {
            "method": "GET",
            "url": "{{base_url}}/orders/order_id"
          }
        },
        {
          "name": "接单",
          "request": {
            "method": "POST",
            "url": "{{base_url}}/orders/order_id/accept",
            "header": [
              {"key": "Authorization", "value": "Bearer {{token}}"}
            ]
          }
        },
        {
          "name": "提交交付",
          "request": {
            "method": "POST",
            "url": "{{base_url}}/orders/order_id/submit",
            "header": [
              {"key": "Content-Type", "value": "application/json"},
              {"key": "Authorization", "value": "Bearer {{token}}"}
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"attachments\": [\"https://...code.zip\", \"https://...doc.pdf\"]\n}"
            }
          }
        },
        {
          "name": "完成订单",
          "request": {
            "method": "POST",
            "url": "{{base_url}}/orders/order_id/complete",
            "header": [
              {"key": "Authorization", "value": "Bearer {{token}}"}
            ]
          }
        }
      ]
    },
    {
      "name": "钱包",
      "item": [
        {
          "name": "获取钱包信息",
          "request": {
            "method": "GET",
            "url": "{{base_url}}/wallet",
            "header": [
              {"key": "Authorization", "value": "Bearer {{token}}"}
            ]
          }
        },
        {
          "name": "获取交易记录",
          "request": {
            "method": "GET",
            "url": "{{base_url}}/wallet/transactions?page=1&limit=20",
            "header": [
              {"key": "Authorization", "value": "Bearer {{token}}"}
            ]
          }
        },
        {
          "name": "发起充值",
          "request": {
            "method": "POST",
            "url": "{{base_url}}/wallet/deposit",
            "header": [
              {"key": "Content-Type", "value": "application/json"},
              {"key": "Authorization", "value": "Bearer {{token}}"}
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"amount\": 1000.00\n}"
            }
          }
        },
        {
          "name": "发起提现",
          "request": {
            "method": "POST",
            "url": "{{base_url}}/wallet/withdraw",
            "header": [
              {"key": "Content-Type", "value": "application/json"},
              {"key": "Authorization", "value": "Bearer {{token}}"}
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"amount\": 500.00,\n  \"bankData\": {\n    \"bankAccount\": \"6217002200058000121\",\n    \"bankName\": \"中国银行\",\n    \"accountHolder\": \"张三\"\n  }\n}"
            }
          }
        }
      ]
    }
  ]
}
